# Plot results

set term png enhanced

set datafile separator ','

set grid

set xlabel "Scale Factor"
set ylabel "IPCD Relative TPS"

#set boxwidth auto
set style fill solid 0.5 border
set style data histogram
set style histogram cluster gap 1
set boxwidth 15

set xtics 1,20

# TODO: Make use of other iterations' data!
plot '< paste -d, postgresql.baseline.csv postgresql.ipcd.csv | tail -n+2' using 1:(100*(($7/$3) - 1)):($7 > $3? 0x00FF00 : 0xFF0000) with boxes notitle lc rgb variable
